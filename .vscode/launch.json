    "version": "0.2.0",
    "configurations": [
        {
            "name": "(tools/syskit-log): run current file (no compression)",
            "type": "Ruby",
            "request": "launch",
            "program": "${file}",
            "includes": ["lib", "test"]
        },
        {
            "name": "(tools/syskit-log): run current file (compression)",
            "type": "Ruby",
            "request": "launch",
            "program": "${file}",
            "includes": ["lib", "test"],
            "env": {
                "SYSKIT_LOG_TEST_COMPRESS": "1"
            }
        }
    ]
}
